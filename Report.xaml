<Window x:Class="KIT206_GUI.Report"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KIT206_GUI"
        mc:Ignorable="d"
        Title="Report" Height="600" Width="450" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" Icon="Group-icon.png">
    <Grid>
        <ListBox x:Name="ReportList" Margin="0,49,0,0" BorderBrush="{x:Null}"/>
        <Button x:Name="CopyEmail" Content="Copy Researchers Email Address" Margin="0,10,0,0" VerticalAlignment="Top" Height="39" Click="CopyEmail_Click"/>


    </Grid>
</Window>
